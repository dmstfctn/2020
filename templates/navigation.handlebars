<nav class="dc-sitenav dc-sitenav__main">
  {{#each navigation}}
    {{#dc_ismenu 'DC' this}}
    <a href="{{dc_urlpath "" }}" data-dc-localtarget="#{{this.slug}}">
      {{this.name}}
      {{> svg_DC }}
    </a>
    {{else}}
    <a href="{{dc_urlpath this.slug }}" data-dc-localtarget="#{{this.slug}}">{{this.name}}</a>
    {{/dc_ismenu}}   
  {{/each}} 
  <a href="{{dc_urlpath "info"}}" data-dc-localtarget="#info">record</a>
</nav>
{{#each navigation}}
  {{> (lookup . 'template') }}
{{/each }}  